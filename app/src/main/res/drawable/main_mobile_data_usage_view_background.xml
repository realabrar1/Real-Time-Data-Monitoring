<?xml version="1.0" encoding="utf-8"?>
<ripple android:color="@color/usage_mobile_ripple"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/item_ripple">
        <shape android:shape="rectangle">
            <solid android:color="@color/mobile_data_usage_background_home"/>
            <corners android:radius="24dp"/>
            <stroke android:color="@color/data_usage_home_stroke"
                android:width="2dp"/>
        </shape>
        <color android:color="@color/mobile_data_usage_background_home"/>
    </item>
</ripple>


